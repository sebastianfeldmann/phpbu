{
  "type": "arangodump",
  "options": {
    "endpoint": "tcp://host:8529",
    "username": "myUserName",
    "password": "mySecretPassword",
    "disableAuthentication": true,
    "database": "myDatabase",
    "dumpData": true,
    "includeSystemCollections": true,
    "collections": "myCollection1,myCollection2",
    "pathToArangodump": "/path/to/custom/bin"
  }
}
